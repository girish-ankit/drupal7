CREATE TABLE `view_query_log` (
  `qid` int(11) NOT NULL AUTO_INCREMENT,
  `view_m_name` varchar(255) DEFAULT NULL,
  `view_h_name` varchar(255) DEFAULT NULL,
  `view_query_data` text,
  `view_run_time` varchar(50) DEFAULT NULL,
  `view_run_path` varchar(255) DEFAULT NULL,
  `view_user_ip` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`qid`)
) ENGINE=MyISAM AUTO_INCREMENT=3 DEFAULT CHARSET=latin1


TRUNCATE TABLE  view_query_log;
